{
    "builds": [{
        "src": "skill-overflow/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "rewrites" : [{
        "source": "/(.*)",
        "destination": "/"
    }],
    "env": {
        "DJANGO_SETTINGS_MODULE": "skill-overflow.settings"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "skill-overflow/wsgi.py"
        }
    ]
}